{
  "$schema": "https://json.schemastore.org/package",
  "name": "sourcedive",
  "version": "0.1.0",
  "packageManager": "pnpm@9.12.0",
  "description": "Sourcedive Analyzer CLI",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "sourcedive": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
